<h1>施設新規登録</h1>
<!-- form_with部分 -->
<%= form_with model: @tennis_court,url: admin_tennis_courts_path do |f| %>
  <h4>画像</h4>
  <%= f.file_field :image, accept: "image/*" %>
  <h4>施設名</h4>
  <%= f.text_field :name %>

  <h4>施設情報</h4>
  <%= f.text_area :explanation %>

  <h4>住所</h4>
  <%= f.text_area :address, autofocus: true %>

  <%= f.submit '投稿' %>
<% end %>